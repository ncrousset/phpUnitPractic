{
    "name": "ncrousset/phpunit",
    "description": "Practica de prueba unitarias",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/dbunit": "^3.0",
        "phpunit/php-invoker": "~1.1.4",
        "phpunit/phpunit": "~6.0.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Rudys Natanael Acosta crousset",
            "email": "natanael926@gmail.com"
        }
    ],
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
